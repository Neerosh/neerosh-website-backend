{
  "version": 2,
  "builds":[
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    { 
      "source": "/(.*)",
      "destination": "/" 
    }
  ],
  "headers": [
      {
          "source": "/(.*)",
          "headers" : [
              {
                  "key" : "Cache-Control",
                  "value" : "no-cache, immutable"
              }
          ]
      }
  ]
}